on boot
    start kernelinit

service kernelinit /sbin/kernelinit.sh
    user root
    seclabel u:r:su:s0
    oneshot
    disabled
    
service sysinit /sbin/sysinit.sh
    class late_start
    user root
    seclabel u:r:init:s0
    oneshot
    disabled

