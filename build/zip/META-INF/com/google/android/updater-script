ui_print(" ");
ui_print("TGPKernel Lite for I9305");
ui_print("------------------------");
ui_print(" ");
ui_print("Flashing Kernel");
package_extract_file("boot.img", "/tmp/boot.img");
write_raw_image("/tmp/boot.img", "/dev/block/mmcblk0p8");
delete("/tmp/boot.img");
ui_print("Done");
ui_print("");
